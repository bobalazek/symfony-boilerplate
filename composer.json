{
    "name": "bobalazek/symfony-boilerplate",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "cravler/maxmind-geoip-bundle": "^1.1",
        "doctrine/annotations": "~1.6",
        "doctrine/cache": "~1.8",
        "doctrine/collections": "~1.5",
        "doctrine/common": "~2.8",
        "doctrine/dbal": "~2.7",
        "doctrine/doctrine-bundle": "~1.10",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/inflector": "~1.3",
        "doctrine/instantiator": "~1.1",
        "doctrine/orm": "^2.6",
        "fluoresce/validate-embedded": "^1.0",
        "gedmo/doctrine-extensions": "^2.4",
        "guzzlehttp/guzzle": "^6.3",
        "incenteev/composer-parameter-handler": "^2.0",
        "jenssegers/agent": "^2.5",
        "jms/di-extra-bundle": "^1.8",
        "jms/serializer-bundle": "^1.1",
        "jms/translation-bundle": "^1.3",
        "knplabs/doctrine-behaviors": "^1.6",
        "knplabs/knp-paginator-bundle": "^2.5",
        "misd/phone-number-bundle": "^1.3",
        "nelmio/cors-bundle": "^1.5",
        "noxlogic/ratelimit-bundle": "^1.7",
        "ocramius/proxy-manager": "~2.2",
        "pelago/emogrifier": "^2.0",
        "picoss/sonata-extra-admin-bundle": "^3.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^5.2",
        "simplethings/entity-audit-bundle": "^1.0",
        "sonata-project/admin-bundle": "^3.48",
        "sonata-project/doctrine-orm-admin-bundle": "^3.8",
        "sonata-project/google-authenticator": "^2.2",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/assetic-bundle": "^2.8",
        "symfony/monolog-bundle": "^3.3",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/symfony": "3.4.*",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.7",
        "twilio/sdk": "^5.21"
    },
    "require-dev": {
        "deployer/deployer": "^6.0",
        "doctrine/data-fixtures": "^1.2",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "platform": {
            "php": "7.2"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    }
}
